{
  "name": "@pump-it-up/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:base-live": "E2E_BASE_LIVE=1 APP_NETWORK=base-sepolia SIGNER_BACKEND=local CDP_MOCK_MODE=false BUNDLER_PRIMARY_NAME=pimlico vitest run --config vitest.e2e.config.ts tests/e2e.local-funding.spec.ts"
  },
  "dependencies": {
    "@fleet/intelligence": "*",
    "@coinbase/cdp-sdk": "^1.29.0",
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "p-limit": "^6.2.0",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "viem": "^2.23.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/pino": "^7.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
